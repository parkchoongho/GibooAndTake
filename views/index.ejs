<!doctype html>
<html lang="en">

<head>
	<style>
		
	</style>
	<!-- Required meta tags -->
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<link rel="icon" href="img/favicon.png" type="image/png">
	<title>
		<% if(loginState){ %>
		<%= email %> <% }else{ %>GIBOO&TAKE <%
				} %>
	</title>
	<!-- Bootstrap CSS -->
	<link rel="stylesheet" href="css/bootstrap.css">
	<link rel="stylesheet" href="vendors/linericon/style.css">
	<link rel="stylesheet" href="css/font-awesome.min.css">
	<link rel="stylesheet" href="vendors/lightbox/simpleLightbox.css">
	<link rel="stylesheet" href="vendors/nice-select/css/nice-select.css">
	<!-- main css -->
	<link rel="stylesheet" href="css/style.css">
	<link rel="stylesheet" href="css/responsive.css">
	<link rel="stylesheet" href="css/modal.css">
	<!-- <link rel="stylesheet" href="css/modal.css"> -->
	<script src="js/jquery-3.2.1.min.js"></script>
	<script src="js/my.js"></script>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>

</head>

<body>

	<!--================ Start Header Menu Area =================-->
	<% include header %>
	<!--================ End Header Menu Area =================-->
	<!-- Login Button -->
	<% include modal %>
	
	<!--================ Home Banner Area =================-->
	<section class="home_banner_area">
		<div class="banner_inner">
			<div class="container">
				<div class="banner_content">
					<p class="upper_text"></p>
					<h2>to make the better world</h2>
					<p>
						당신의 가치있는 소비가 이 세상을 아름답게 만듭니다.
					</p>
					<a class="primary_btn mr-20" href="#">정기후원하기</a>
					<a class="primary_btn yellow_btn text-white" href="#">일시후원히기</a>


				</div>
			</div>
		</div>
	</section>
	<!--================ End Home Banner Area =================-->

	<!--================ Start icon Area =================-->
	<section class="causes_area">
		<div class="container">
		
			<div class="row">
				<div>
					<div class="myicon">
						<img id="main" src="img/main.png" width="100" height="100">
					</div>
					<h5>전체테마</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="kid" src="img/baby.png" width="100" height="100">
					</div>
					<h5>아동/청소년</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="elder" src="img/people.png" width="100" height="100" alt="">
					</div>
					<h5>어르신</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="world" src="img/world.png" width="100" height="100" alt="">
					</div>
					<h5>지구촌</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="people" src="img/people1.png" width="100" height="100" alt="">
					</div>
					<h5>장애인</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="animal" src="img/dog.png" width="100" height="100" alt="">
					</div>
					<h5>동물</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="nature" src="img/nature.png" width="100" height="100" alt="">
					</div>
					<h5>자연</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="grandma" src="img/woman.png" width="100" height="100" alt="">
					</div>
					<h5>위안부할머니</h5>
				</div>
				<div>
					<div class="myicon">
						<img id="etc" src="img/etc.png" width="100" height="100" alt="">
					</div>
					<h5>기타</h5>
				</div>

			</div>
	</section>
	<!--================ End icon Area =================-->

	<!--================ Start category section =================-->
	<div class="container" id="category_result">
		<% include category/main %>
	</div>
   	<!--================ End category section =================-->
	   
	<!--================ Start foundation section =================-->
	<% include foundation %>
	<!--================ End CTA Area =================-->

	<!--================ Start CTA Area =================-->
	<% include text %>
	<!--================ End CTA Area =================-->

	<!--================ Start Story Area =================-->
	<% include story %>
	<!--================ End Story Area =================-->
	<% include footer %>
	


	<!-- Optional JavaScript -->
	<!-- jQuery first, then Popper.js, then Bootstrap JS -->
	<script src="js/jquery-3.2.1.min.js"></script>
	<script src="js/popper.js"></script>
	<script src="js/bootstrap.min.js"></script>
	<script src="js/stellar.js"></script>
	<script src="vendors/lightbox/simpleLightbox.min.js"></script>
	<script src="vendors/nice-select/js/jquery.nice-select.min.js"></script>
	<script src="js/jquery.ajaxchimp.min.js"></script>
	<script src="js/mail-script.js"></script>
	<script src="js/countdown.js"></script>
	<!--gmaps Js-->
	<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCjCGmQ0Uq4exrzdcL6rvxywDDOvfAu6eE"></script>
	<script src="js/gmaps.min.js"></script>
	<script src="js/theme.js"></script>
